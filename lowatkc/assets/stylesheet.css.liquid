}
#cart .dropdown-menu:after,#cart .dropdown-menu:before {
  border-bottom-style: solid;
  content: '';
  display: block;
  height: 0;
  position: absolute;
  width: 0;
}
#cart .dropdown-menu:before {
  border: 7px solid;
  border-color: transparent transparent #fff transparent;
  z-index: 99;
  right: 26px;
  top: -12px;
  opacity: 0;
}
#cart .dropdown-menu:after {
  border: 8px solid;
  border-color: transparent transparent #fff transparent;
  z-index: 98;
  right: 25px;
  top: -15px;
}
