<div class="container">
  {% include 'breadcrumb' %} 
  {% if section.settings.page_top != blank %}
  <div class="has-thumb">
    <script type="text/javascript">
      $('#header-top').css("background-image", "url({{ section.settings.page_top | img_url: 'master' }})");
    </script>
  </div>
  {% endif %}
  <div class="row">
    <div id="content" class="col-sm-12" >
      <div class="row">
        {% include 'about-page-section' %}
        {{ page.content }}
      </div>
    </div>
  </div>
</div>

